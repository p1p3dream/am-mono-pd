{{- $config := datasource "config" -}}
{{- $endpoints := $config.endpoints -}}
{{- $env := datasource "env" -}}
{{- $local_ci := eq (env.Getenv "LOCAL_CI") "true" -}}

# Code generated by gomplate. DO NOT EDIT.

{{ if env.Getenv "SENTRY_DSN" }}
sentry:
  dsn: "{{ env.Getenv "SENTRY_DSN" }}"
{{ end }}

{{ if index $env "ABODEMINE_DATAPIPE_FLAGS" }}
flags:
{{- range strings.Split "," $env.ABODEMINE_DATAPIPE_FLAGS }}
  - "{{ . | strings.TrimSpace }}"
{{- end }}
{{ end }}
