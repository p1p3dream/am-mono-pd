{{- $config := datasource "config" -}}

{{- $user_data := $config.vars.user_data -}}

# Code generated by gomplate. DO NOT EDIT.

StrictHostKeyChecking no

Host *.amazonaws.com
    AddressFamily inet

Host github.com
    HostName github.com
    IdentityFile /home/{{ $user_data.user.user }}/.ssh/{{ $user_data.ssh.ed25519 }}
    IdentitiesOnly yes
